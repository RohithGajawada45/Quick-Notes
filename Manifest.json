{
    "manifest_version": 2,
    "name": "Quick Notes",
    "description": "A simple note-taking extension for Chrome.",
    "version": "1.0",
    "permissions": ["storage"],
    "browser_action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "./icons/notes.png",
        "128": "./icons/notes-2.png"
      }
    },
    "icons": {
      "16": "./icons/notes.png",
      "128": "./icons/notes-2.png"
    }
  }
  